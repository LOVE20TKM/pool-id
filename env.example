# Private key for deployment and transactions
PRIVATE_KEY=your_private_key_here

# LOVE20 Token address (required for deployment)
LOVE20_TOKEN_ADDRESS=0x...

# Pool ID contract address (required for interaction scripts)
POOL_ID_ADDRESS=0x...

# Pool name for minting
POOL_NAME=YourPoolName

# Token ID for querying
TOKEN_ID=1

# Pool name to calculate mint cost
CALCULATE_FOR=ExampleName

# RPC URL for the network
RPC_URL=https://your-rpc-url-here

# Etherscan API key for verification (optional)
ETHERSCAN_API_KEY=your_api_key_here

